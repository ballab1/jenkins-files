<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>5</daysToKeep>
        <numToKeep>5</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>CONTAINER_DIR</name>
          <description></description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>prod/alpinefull</string>
              <string>prod/hubot</string>
              <string>prod/jenkins</string>
              <string>prod/mysql</string>
              <string>prod/nagios</string>
              <string>prod/nginx</string>
              <string>prod/phpadmin</string>
              <string>prod/zen</string>
              <string>dev/helloworld</string>
              <string>dev/hubot</string>
              <string>dev/node</string>
              <string>dev/nodervisor</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>ubuntu-s3</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>/home/bobb/prod</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>[[ $(docker ps -a --filter &quot;name=_&quot; --format &quot;{{.Names}}&quot;) ]] &amp;&amp; \
        docker rm -f $(docker ps -a --filter &quot;name=_&quot; --format &quot;{{.Names}}&quot;)
[[ $(docker ps -a --filter &quot;status=exited&quot; --filter &quot;status=dead&quot; --format &quot;{{.Names}}&quot;) ]] &amp;&amp; \
        docker rm $(docker ps -a --filter &quot;status=exited&quot; --filter &quot;status=dead&quot; --format &quot;{{.Names}}&quot;)
[[ $(docker images -f &quot;dangling=true&quot; -q) ]] &amp;&amp; \
        docker rmi $(docker images -f &quot;dangling=true&quot; -q)

docker-compose -f &quot;/home/bobb/$CONTAINER_DIR/docker-compose.yml&quot; build</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <ie.googlielmo.publishtokafka.PublishToKafkaBuilder plugin="publishtokafka@1.0">
      <bootstrapServers>broker:9092,10.1.3.11:9092</bootstrapServers>
      <metadataBrokerList>broker:9092,10.1.3.11:9092</metadataBrokerList>
      <acks>1</acks>
      <topic>jenkins_data</topic>
      <excludePlugin>false</excludePlugin>
      <changeBuildStatus>false</changeBuildStatus>
      <retries>0</retries>
      <clientId></clientId>
      <connectionsMaxIdle>540000</connectionsMaxIdle>
      <requestTimeout>30000</requestTimeout>
      <timeout>30000</timeout>
      <metadataFetchTimeout>60000</metadataFetchTimeout>
      <metadataMaxAge>300000</metadataMaxAge>
      <fetchUnitTestsResults>false</fetchUnitTestsResults>
    </ie.googlielmo.publishtokafka.PublishToKafkaBuilder>
  </publishers>
  <buildWrappers>
    <org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper plugin="credentials-binding@1.15">
      <bindings class="empty-list"/>
    </org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper>
    <hudson.plugins.timestamper.TimestamperBuildWrapper plugin="timestamper@1.8.9"/>
    <hudson.plugins.ansicolor.AnsiColorBuildWrapper plugin="ansicolor@0.5.2">
      <colorMapName>gnome-terminal</colorMapName>
    </hudson.plugins.ansicolor.AnsiColorBuildWrapper>
  </buildWrappers>
</project>